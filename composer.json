{
  "name": "nedgen/cakephp-soldo",
  "description": "Soldo plugin for CakePHP",
  "keywords": [
    "cakephp",
    "soldo",
    "plugin",
    "php",
    "api"
  ],
  "version": "1.2.0",
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "cakephp/cakephp": "^3.5",
    "muffin/webservice": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.14|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Soldo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
    }
  }
}
